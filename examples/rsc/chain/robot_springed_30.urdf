<robot name="2DrobotArm">
    <link name="world"/>

    <link name="link1">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <mass value="1"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
        </inertial>
    </link>

    <joint name="worldTolink1" type="fixed">
        <parent link="world"/>
        <child link="link1"/>
        <origin xyz="0 0 10"/>
    </joint>

    <link name="link2">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <mass value="1"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
        </inertial>
    </link>

    <joint name="link1Tolink2" type="spherical">
        <parent link="link1"/>
        <child link="link2"/>
        <origin xyz="0 0 -0.24"/>
        <axis xyz="0 1 0"/>
        <dynamics rotor_inertia="0.0001" spring_mount="0.70710678118 0 0.70710678118 0" stiffness="500.0" damping="3."/>
    </joint>

    <link name="link3">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <mass value="1"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
        </inertial>
    </link>

    <joint name="link2Tolink3" type="spherical">
        <parent link="link2"/>
        <child link="link3"/>
        <origin xyz="0 0 -0.24"/>
        <axis xyz="0 1 0"/>
        <dynamics spring_mount="0.70710678118 0.70710678118 0 0" stiffness="500.0" damping="3."/>
    </joint>

    <link name="link4">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <geometry>
                <sphere radius="0.1"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 -0.12"/>
            <mass value="1"/>
            <inertia ixx="0.002" ixy="0.0" ixz="0" iyy="0.002" iyz="0" izz="0.002"/>
        </inertial>
    </link>

    <joint name="link3Tolink4" type="spherical">
        <parent link="link3"/>
        <child link="link4"/>
        <origin xyz="0 0 -0.24"/>
        <axis xyz="0 1 0"/>
        <dynamics spring_mount="0.70710678118 0 0.70710678118 0" stiffness="500.0" damping="3."/>
    </joint>

</robot>
